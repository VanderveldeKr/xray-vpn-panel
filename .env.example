# Конфигурация VPN сервера
# Скопируйте этот файл в .env и заполните своими данными

# Домен вашего сервера (ОБЯЗАТЕЛЬНО!)
DOMAIN=your-domain.com

# Пароль для админ-панели (ОБЯЗАТЕЛЬНО ИЗМЕНИТЕ!)
ADMIN_PASSWORD=your_secure_password_here

# Email для Let's Encrypt сертификатов
LETSENCRYPT_EMAIL=your-email@example.com

# Часовой пояс
TZ=Europe/Moscow

# Порты (по умолчанию)
XRAY_VISION_PORT=8443
XRAY_WS_PORT=8444
HTTP_PORT=80
HTTPS_PORT=443
